@model MvcCv.Models.Entity.TblYeteneklerim


@{
    ViewBag.Title = "YetenekDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h3>Yetenek Düzenleme Sayfası</h3>
<form method="post">

    <br />
    @Html.Label("Yetenek Açıklaması")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-success">Güncelle</button>
    <a href="/Yetenek/Index/" class="btn btn-outline-primary">Geri Dön</a>
</form>
